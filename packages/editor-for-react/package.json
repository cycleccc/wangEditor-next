  "type": "module",
  "homepage": "https://github.com/cycleccc/wangEditor-next#readme",
  "license": "MIT",
  "types": "dist/editor-for-react/src/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/editor-for-react/src/index.d.ts",
